	public void connect()
	{
		URL url;
		try {
			String str="https://www.indeed.com/q-semiconductor-jobs.html"; 
			
			url = new URL(str);
			// open url
			URLConnection con = (URLConnection)url.openConnection();  
		    // the the stream
		   //InputStream input = con.getInputStream();  
		   
		} catch (MalformedURLException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		} catch (IOException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}  
  

  
	}
}
